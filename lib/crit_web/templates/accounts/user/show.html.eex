<h1>Show User</h1>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @user.display_name %>
  </li>

  <li>
    <strong>Login id:</strong>
    <%= @user.auth_id %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>

  <li>
    <strong>Active:</strong>
    <%= @user.active %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.accounts_user_path(@conn, :edit, @user) %></span>
<span><%= link "Back", to: Routes.accounts_user_path(@conn, :index) %></span>
